{
  "name": "generator-alfred-python",
  "version": "0.0.0",
  "description": "Generator to scaffold out a new Alfred workflow using python",
  "homepage": "",
  "author": {
    "name": "Matthew McGarvey",
    "email": "tnraptor14@hotmail.com",
    "url": "https://github.com/matthewmcgarvey"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "alfred",
    "python",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "matthewmcgarvey/generator-alfred-python",
  "license": "Apache-2.0"
}
